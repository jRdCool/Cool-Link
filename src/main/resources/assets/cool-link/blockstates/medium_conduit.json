{
  "multipart": [
    { "when": { "OR": [
      {"north": "false", "east": "false", "south": "false", "west": "false","junctionbox": "false"}
    ]},
      "apply": { "model": "cool-link:block/medium_conduit", "uvlock": true }
    },
    { "when": { "OR": [
      { "north": "true","junctionbox": "false" },
      { "north": "false", "east": "false", "south": "true", "west": "false" }
    ]},
      "apply": { "model": "cool-link:block/medium_conduit", "uvlock": true }
    },
    {   "when": { "OR": [
      { "south": "true","junctionbox": "false" },
      { "north": "true", "east": "false", "south": "false", "west": "false" }
    ]},
      "apply": { "model": "cool-link:block/medium_conduit", "uvlock": true }
    },
    {   "when": { "OR": [
      { "west": "true","junctionbox": "false" },
      { "north": "false", "east": "true", "south": "false", "west": "false" }
    ]},
      "apply": { "model": "cool-link:block/medium_conduit", "y":  90 }
    },
    {   "when": { "OR": [
      { "east": "true","junctionbox": "false" },
      { "north": "false", "east": "false", "south": "false", "west": "true" }
    ]},
      "apply": { "model": "cool-link:block/medium_conduit", "y":  90 }
    },



    {   "when": { "OR": [
      {"north": "true", "east": "true", "south": "true", "west": "false"},
      {"junctionbox": "true"}
    ]},
      "apply": { "model": "cool-link:block/conduit_junction_box", "uvlock": true }
    },
    {   "when": { "OR": [
      {"north": "false", "east": "true", "south": "true", "west": "true"},
      {"junctionbox": "true"}
    ]},
      "apply": { "model": "cool-link:block/conduit_junction_box", "uvlock": true }
    },
    {   "when": { "OR": [
      {"north": "true", "east": "false", "south": "true", "west": "true"},
      {"junctionbox": "true"}
    ]},
      "apply": { "model": "cool-link:block/conduit_junction_box", "uvlock": true }
    },
    {   "when": { "OR": [
      {"north": "true", "east": "true", "south": "false", "west": "true"},
      {"junctionbox": "true"}
    ]},
      "apply": { "model": "cool-link:block/conduit_junction_box", "uvlock": true }
    },
    {   "when": { "OR": [
      {"north": "true", "east": "true", "south": "true", "west": "true"},
      {"junctionbox": "true"}
    ]},
      "apply": { "model": "cool-link:block/conduit_junction_box", "uvlock": true }
    }

  ]
}